.van-button--normal.data-v-1edf4007 {
  padding: 0 15px;
  font-size: 14px;
}
.van-button.data-v-1edf4007::before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  background-color: #000;
  border: inherit;
  border-color: #000;
  border-radius: inherit;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  opacity: 0;
  content: ' ';
}
.van-button--block.data-v-1edf4007 {
  display: block;
  width: 100%;
}
.van-button__content.data-v-1edf4007 {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}
.van-button--normal.data-v-1edf4007 {
  padding: 0 15px;
  font-size: 14px;
}
.van-button--primary.data-v-1edf4007 {
  color: #fff;
  background-color: #07c160;
  border: 1px solid #07c160;
}
.van-cell.data-v-1edf4007 {
  position: relative;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  padding: 10px 16px;
  overflow: hidden;
  color: #323233;
  font-size: 14px;
  line-height: 24px;
  background-color: #fff;
}
.van-cell.data-v-1edf4007::after {
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  right: 16px;
  bottom: 0;
  left: 16px;
  border-bottom: 1px solid #ebedf0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
